{
    "name": "humi/json-api-connector",
    "description": "Allows for simple API communication with a JsonApi compatible endpoint",
    "authors": [
        {
            "name": "Humi",
            "email": "dev@humi.ca"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "8.x-dev",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Humi\\JsonApiConnector\\": "src/",
            "Humi\\JsonApiConnector\\Tests\\": "tests/"
        }
    }
}
